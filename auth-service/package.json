{
  "name": "restaurant-auth-service",
  "version": "1.0.0",
  "description": "Servicio de autenticaci√≥n para el sistema de restaurantes",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
